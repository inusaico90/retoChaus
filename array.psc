Algoritmo EJArray
	Definir i como real;
	Definir a Como Caracter;
	i<-0;
	Escribir "Define la longitud:";Leer i;
	Limpiar Pantalla;
	Dimension a[i];
	Para i<-0 Hasta i-1 Con Paso 1 Hacer
		Escribir "Vector: ",i;
		Leer a[i];
	Fin Para
	Escribir "¿Qué valor quieres ver?";
	Leer i;
	Escribir a[i-1];
